@Article{SurvHybridCRNsYu2014,
  Title                    = {The Security of Physical Layer in Cognitive Radio Networks},
  Author                   = {Yi-cheng Yu, Liang Hu, Hong-tu Li, Yuan-mo Zhang, Fang-ming Wu, and Jian-feng Chu},
  Journal                  = {Journal of Communications},
  Year                     = {2014},

  Month                    = {December},
  Number                   = {12},
  Pages                    = {916-922},
  Volume                   = {9},

  Abstract                 = {Cognitive Radio (CR) is a novel technology that promises to solve the lack of spectrum by allowing secondary users to use licensed band, so that they can coexist with primary users without causing interference to their communications. With the development of CR, extending to the level of network, cognitive radio networks (CRNs) emerge as the times require. Nowadays the operational aspects of CRN are being explored vigorously, and several potential security challenges for cognitive radio have gained lots of attention. In this paper, we explore the security issues on physical layer for cognitive radio networks. First, we give a brief overview of the CRNs, then we review several existing secure threats to the physical layer in CRNs and we propose a new kind of security problem. Next, we discuss the related countermeasures on how to defend against these attacks. Subsequently, we conduct an evaluation of these countermeasures, and make some future works for secure CRNs. At last, we make the conclusion.},
  Owner                    = {egaebel},
  Review                   = {Problem: Survey of security issues in various types of cognitive radio networks.\\ \\ Solution: This paper hits on many familiar security issues in cognitive radio networks, but contains a few new perspectives. The most interesting of these perspectives is distinguishing between objective function attacks, which temporarily impair a cognitive radio's service quality, and a learning attack, which inflicts long term damage on the cognitive radio's cognition cycle. In addition, this survey takes into account three different varieties of cognitive radio networks: peer-to-peer, infrastructure driven (like cellular networks), and (in my opinion the most likely scenario) mesh networks which are a mix between the two previous varieties. \\ Why Did I Read It?: To survey the style in which cognitive radio survey papers are being written as well as to find a new resource for discovering new papers relating to cognitive radio security. \\ \\},
  Timestamp                = {2015.01.16}
}

@Article{AntiJammingWu2012,
  Title                    = {Anti-Jamming Games in Multi-Channel Cognitive Radio Networks},
  Author                   = {Yongle Wu, and Beibei Wang, K. J. Ray Liu, and T. Charles Clancy},
  Journal                  = {IEEE Journal on Selected Areas in Communications},
  Year                     = {2012},

  Month                    = {January},
  Number                   = {1},
  Volume                   = {30},

  Abstract                 = {Crucial to the successful deployment of cognitive radio networks, security issues have begun to receive research interests recently. In this paper, we focus on defending against the jamming attack, one of the major threats to cognitive radio networks. Secondary users can exploit the flexible access to multiple channels as the means of anti-jamming defense. We first investigate the situation where a secondary user can access only one channel at a time and hop among different channels, and model it as an anti-jamming game. Analyzing the interaction between the secondary user and attackers, we derive a channel hopping defense strategy using the Markov decision process approach with the assumption of perfect knowledge, and then propose two learning schemes for secondary users to gain knowl- edge of adversaries to handle cases without perfect knowledge. In addition, we extend to the scenario where secondary users can access all available channels simultaneously, and redefine the anti-jamming game with randomized power allocation as the defense strategy. We derive the Nash equilibrium for this Colonel Blotto game which minimizes the worst-case damage. Finally, simulation results are presented to verify the performance.},
  Owner                    = {egaebel},
  Review                   = {Problem: Cognitive radios, and indeed any radio, is subject to jamming.\\ \\ Solution: Use game theoretic techniques and learning to develop strategies for changing radio parameters to avoid or run from jammers. The authors create several different game models to fit different scenarios. First, they create an anti-jamming game, where utility is gained from communication and a certain amount of utility is lost from having to hop channels. The authors then examine how Markov decision processes (MDPs) can be used to reason about strategies for this game. The MDP model assumes perfect knowledge Second, the authors examine feeding the MDP with learned knowledge from experience. The authors examine how well MLE-Based learning works and how well Q-Learning works. Third, the authors formulate a new game with enough radios to cover all channels but a constrained power budget; the power allocation game examines how a secondary user should allocate power to minimize jamming. \\ \\ Why Did I Read It?: I am aware that game theory is a popular paradigm to apply to cognitive radios, I decided I should examine it to see if I can find any security implications. Since the authors are using learning it is my hope that I can find a way to attack this approach to instill malicious or false beliefs into the learning engine. \\ \\ Other Notes: The authors give very rigorous proofs and models in this paper and it is very thorough. This is definitely a paper I will come back to reference again.\\},
  Timestamp                = {2014.12.12}
}

@Article{SpecSenseSurveyYucek2009,
  Title                    = {A Survey of Spectrum Sensing Algorithms for Cognitive Radio Applications},
  Author                   = {Tevfik Yucek and Huseyin Arslan},
  Journal                  = {IEEE Communications Surveys and Tutorials},
  Year                     = {2009},
  Number                   = {1},
  Pages                    = {116-130},
  Volume                   = {11},

  Abstract                 = {The spectrum sensing problem has gained new aspects with cognitive radio and opportunistic spectrum access concepts. It is one of the most challenging issues in cognitive radio systems. In this paper, a survey of spectrum sensing methodologies for cognitive radio is presented. Various aspects of spectrum sensing problem are studied from a cognitive radio perspective and multi-dimensional spectrum sensing concept is introduced. Challenges associated with spectrum sensing are given and enabling spectrum sensing methods are reviewed. The paper explains the cooperative sensing concept and its various forms. External sensing algorithms and other alternative sensing methods are discussed. Furthermore, statistical modeling of network traffic and utilization of these models for prediction of primary user behavior is studied. Finally, sensing features of some current wireless standards are given.},
  Owner                    = {egaebel},
