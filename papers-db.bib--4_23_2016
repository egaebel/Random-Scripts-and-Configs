@InProceedings{DiscrimGenClassNg2001,
  Title                    = {On Discriminative vs. Generative Classifiers: A comparison of logistic regression and naive Bayes},
  Author                   = {Andrew Y. Ng, Michael I. Jordan},
  Booktitle                = {Advances in Neural Information Processing Systems (NIPS)},
  Year                     = {2001},
  Number                   = {14},

  Owner                    = {egaebel},
  Review                   = {Problem: When to use discriminative vs. generative classifiers?\\ \\ Solution: The authors show through analysis and experiment that generative classifiers approach their (higher) asymptotic error much faster than the discriminative classifiers reach thair (lower) error. As a result, the choice to use one or the other is dependent on the training set size; generative classifiers do better for smaller training sets.\\ \\ Why Did I Read It?: I found myself mixing up discriminative and generative classifiers in the course of my machine learning education. Several different searches kept turning this paper up on the definitive guide on the two, so I read it.\\ \\ Other Notes: My purpose in reading this paper was served, I now have a solid idea of the difference between the two.\\ \\},
  Timestamp                = {2015.01.22}
}

