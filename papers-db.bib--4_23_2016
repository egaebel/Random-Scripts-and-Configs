@Article{SimplePairingAnalysisBluetooth4Phan2010,
  Title                    = {Analyzing the Secure Simple Pairing in Bluetooth v4.0},
  Author                   = {Phan, Raphael C.-W. and Mingard, Patrick},
  Journal                  = {Wireless Personal Communications},
  Year                     = {2010},
  Number                   = {4},
  Pages                    = {719--737},
