@Article{PhyLayerSecSurvHe2013,
  Title                    = {Wireless Physical Layer Security with Imperfect Channel State Information: A Survey},
  Author                   = {Biao He, Xiangyun Zhou, Thushara D. Abhayapala},
  Journal                  = {Arxiv},
  Year                     = {2013},

  Abstract                 = {Physical layer security is an emerging technique to improve the wireless communication security, which is widely regarded as a complement to cryptographic technologies. To design physical layer security techniques under practical scenarios, the uncertainty and imperfections in the channel knowledge need to be taken into consideration. This paper provides a survey of recent research and development in physical layer security considering the imperfect channel state information (CSI) at communication nodes. We first present an overview of the main information-theoretic measures of the secrecy performance with imperfect CSI. Then, we describe several signal processing enhancements in secure transmission designs, such as secure on-off transmission, beamforming with artificial noise, and secure communication assisted by relay nodes or in cognitive radio systems. The recent studies of physical layer security in large-scale decentralized wireless networks are also summarized. Finally, the open problems for the on-going and future research are discussed.},
  Owner                    = {egaebel},
  Review                   = {A survey on physical layer security techniques. Includes an interesting discussion of estimating channel state information (CSI) at receiver and eavesdropper as well as using the CSI in determining thresholds for security.},
  Timestamp                = {2015.04.01}
}

@Article{RealitiesLe2007,
  Title                    = {Cognitive Radio Realities},
  Author                   = {Bin Le, and Thomas W. Rondeau, and Charles W. Bostian},
  Journal                  = {Wireless Communications and Mobile Computing},
  Year                     = {2007},

  Month                    = {May},
  Number                   = {9},
  Pages                    = {1037-1048},
  Volume                   = {7},

  Abstract                 = {A cognitive radio (CR) is a transceiver that is aware, adaptive, and capable of learning. It may be visualized and realized as an intelligent software package (cognitive engine) controlling a software defined radio platform. The cognitive engine executes a set of nested loops constituting a cognition cycle, drawing on experience and stored knowledge to optimize a set of user-chosen quality of service measures. The process relies on the effectiveness of a set of tools that are individually well known but rarely found together: multi-objective genetic algorithms, case-based decision theory, and neural networks. Practical implementation problems include passing environmental information from the radio to the cognitive engine, acting on that information, and performing real-time control of the radio platform by the cognitive engine. In this paper, we discuss our approach to developing and implementing a CR.},
  Owner                    = {egaebel},
  Review                   = {Problem: Developing a cognitive engine for a cognitive radio which the authors define as being "a transceiver that is aware, adaptive, and capable of learning from experience. (this paper seems to be a survey paper that is more targetted than usual) Solution: The authors describe a high level architecture for a cognitive radio. They then go further by discussing specific implementation features for a cognitive engine in a cognitive radio such as using evolutionary techniques (genetic algorithms specifically) in the engine. The authors then discuss the problem that will arise of identifying particular signals when their channel parameters are unknown before communication time. They discuss applying artificial neural networks to his problem. The authors close by claiming that cognitive engines will continue to rise up the network stack. Why Did I Read It?: I want to read earlier works in cognitive radio to get a better clue of the progression of the field. I think this will be useful to both give me ideas and to eliminate ideas that have already been eliminated. Other Notes: I found this paper very rewarding. It was well written. It provided several cognitive radio conference names that I had not previously known about. It mentions a concrete implementation of a cognitive radio system that runs using GNU Radio which we have available to us! The cognitive radio implementation uses genetic algorithms in its cognitive engine. I intend to find this engine, download it, examine it, and see if it is possible to maliciously influence the cognitive engine into misbehaving.},
  Timestamp                = {2014.12.17}
}

