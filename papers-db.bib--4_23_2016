@Article{LimitCryptoSigAuthJiang2012,
  Title                    = {On the Limitation of Embedding Cryptographic Signature for Primary Transmitter Authentication},
  Author                   = {Tingting Jiang et al.},
  Journal                  = {IEEE Wireless Communications Letters},
  Year                     = {2012},

  Month                    = {August},
  Number                   = {4},
  Volume                   = {1},

  Abstract                 = {Recently, an interesting primary transmitter authentication scheme was proposed. The main idea of this scheme is to have the primary transmitter embed cryptographic authentication tag at the physical layer. There are a number of features that make this scheme attractive. In this paper, we investigate the effective coverage areas for the primary and secondary receivers before and after applying this scheme. During the process, we reveal a serious limitation of this scheme, which may prohibit its application in practice.},
  Owner                    = {egaebel},
  Review                   = {Problem: Ensuring adequate service coverage to primary and secondary users in spectrum sharing scenarios while authenticating the primary user signals.\\ \\ Solution: This paper points out an important limitation of the often proposed cryptographic watermarking for primary user authentication. The paper analyzes the bit-error rate and the effective service coverage of signals having cryptographic symbols embedded, both in comparison to signals with no embedding. The paper finds that at least 65% of secondary users will be unable to authenticate primary users due to the signal degradation caused by the cryptographic embedding. This paper is EXTREMELY important as it shows a very simple analysis of the limitations of cryptographic embedding, which seems VERY appealing if you listen to those who propose it in papers, with apparently no thought given to practical applications.\\ \\},
  Timestamp                = {2015.02.13}
}

