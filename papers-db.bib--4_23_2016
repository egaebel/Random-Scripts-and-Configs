@Article{LearningSecCogRadioBahrak2012,
  Title                    = {Security of Spectrum Learning in Cognitive Radios},
  Author                   = {Benham Bahrak and Jung-Min "Jerry" Park},
  Journal                  = {SK Telecom Telecommunications Review},
  Year                     = {2012},

  Month                    = {December},
  Number                   = {6},
  Pages                    = {850-864},
  Volume                   = {22},

  Abstract                 = {Due to delay and energy constraints, a cognitive radio may not be able to perform spectrum sensing in all available channels. Therefore, a sensing policy is needed to decide which channels to sense. The channel selection problem is the problem of designing such a sensing policy to maximize throughput while avoiding interference to primary users. The channel selection problem can be formulated as a reinforcement learning problem. Channel selection schemes that employ reinforcement machine learning algorithms are vulnerable to belief manipulation attacks that contaminate the knowledge base of the learning algorithms. In this paper, we analyze the security of channel selection algorithms that are based on reinforcement learning and propose mitigation techniques that make these algorithms more robust against belief manipulation attacks.},
  Owner                    = {egaebel},
  Review                   = {Problem: Optimally attacking cognitive radios that use reinforcement learning in their cognitive engines via belief manipulation attacks.\\ \\ Solution: The authors devise many optimization problems for attackers to solve to identify optimal QoS degradation assuming the attacker can only jam one channel at a time. The authors show a modest decrease in service when applying their techniques to two reinforcement learning strategies: myopic policy and softmax policy, which are used in other papers referenced. Most of the mathematics involved in the optimization problems went right over my head, however it is my hope that during and certainly following this semester (Spring 2015) I will be able to fully understand all of the optimization problems posed as a result of my optimization laden coursework.\\ \\ Why Did I Read It?: This is the first paper I have found that addresses the topic in cognitive radio that has primarily piqued my interest: adversarial learning applied to cognitive engines.\\ \\ Other Notes: I predict that I will reference this paper quite a bit over the next year.\\ \\},
  Timestamp                = {2015.01.15}
}

